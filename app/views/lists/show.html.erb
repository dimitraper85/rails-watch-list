<div class="container">
  <h2><%= @list.name %></h2>
  <div>
    <ul> <% @bookmarks.each do |bookmark| %>
            <li><%= bookmark.movie.title %></li>
            <p><%= bookmark.comment %></p>
            <%= link_to "Delete this bookmark", bookmark_path(bookmark),
            data:{turbo_method: :delete, turbo_confirm: "Are you sure?"}%>
          <% end %>
    </ul>
  </div>
</div>
